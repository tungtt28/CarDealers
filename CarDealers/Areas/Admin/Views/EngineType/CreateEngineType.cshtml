@model CarDealers.Entity.EngineType
@{
    ViewBag.Title = "Create Engine Type";
}
<div class="content-wrap">
    <div class="col-lg-12">
        <div class="card">
            @using (Html.BeginForm("CreateEngineType", "EngineType", FormMethod.Post))
            {
                @Html.AntiForgeryToken()

                <div class="form-group">
                    @Html.LabelFor(model => model.EngineTypeName)
                    @Html.TextBoxFor(model => model.EngineTypeName, new { @class = "form-control input-rounded" })
                    <div style="color : red">
                        @Html.ValidationMessageFor(model => model.EngineTypeName)
                    </div>
                </div>

                    <div class="form-group">
                    @Html.LabelFor(model => model.Description)
                    @Html.TextBoxFor(model => model.Description, new { @class = "form-control input-rounded" })
                        <div style="color : red">
                        @Html.ValidationMessageFor(model => model.Description)
                        </div>
                    </div>

                <!-- Add other form fields as needed -->

                <button type="submit" class="btn btn-dark">Create EngineType</button>
            }
        </div>
        <a style="float:right" class="btn btn-dark" href="~/admin/EngineType/ListEngineType">Back to list</a>
    </div>
</div>
